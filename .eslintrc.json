{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "html"
  ],
  "settings": {
    "html/indent": "+2"
  },
  "indent": [
    2,
    4
  ],
  "linebreak-style": [
    1,
    "windows"
  ],
  "no-console": [
    0
  ],
  "comma-dangle": [
    0
  ],
  "no-undef": [
    0
  ],
  "quotes": [
    1,
    "single"
  ],
  "semi": [
    2,
    "always"
  ]
}